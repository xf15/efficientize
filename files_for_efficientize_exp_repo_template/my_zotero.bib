@misc{brohl_visual_2022,
  title = {Visual and Auditory Cortices Represent Acoustic Speech-Related Information during Silent Lip Reading},
  author = {Br{\"o}hl, Felix and Keitel, Anne and Kayser, Christoph},
  year = {2022},
