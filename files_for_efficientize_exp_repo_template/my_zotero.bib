@article{magnuson_dynamics_2007,
  title = {The {{Dynamics}} of {{Lexical Competition During Spoken Word Recognition}}},
  author = {Magnuson, James S. and Dixon, James A. and Tanenhaus, Michael K. and Aslin, Richard N.},
