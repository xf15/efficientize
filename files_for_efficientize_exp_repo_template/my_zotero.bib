@inproceedings{zhao_pose_2018,
  title = {Towards {{Pose Invariant Face Recognition}} in the {{Wild}}},
  booktitle = {2018 {{IEEE}}/{{CVF Conference}} on {{Computer Vision}} and {{Pattern Recognition}}},
