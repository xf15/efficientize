  langid = {english},
  file = {/Users/xzfang/Zotero/storage/EUZGP5S2/Han et al. - 2020 - Scale and translation-invariance for novel objects.pdf;/Users/xzfang/Zotero/storage/4XE65U5W/s41598-019-57261-6.html}
